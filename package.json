    "jsdom": "^11.11.0",
    "json": "^9.0.6",
    "mini-css-extract-plugin": "^0.4.1",
    "nock": "^9.4.2",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.6.3",
    "react-html-parser": "^2.0.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redis-mock": "^0.42.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "testcafe": "^0.23.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
