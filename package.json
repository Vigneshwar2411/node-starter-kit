    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-testcafe": "^0.2.1",
    "foundation-sites": "^6.4.3",
    "history": "^4.7.2",
    "husky": "^1.3.1",
